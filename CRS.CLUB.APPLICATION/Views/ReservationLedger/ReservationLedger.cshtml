                                        <label class="block mb-[12px] text-sm font-medium text-secondary-100 ">
                                            @CRS.CLUB.APPLICATION.Resources.Resource.From_date
                                        </label>
                                        <input type="text" class="w-full" id="fromdatepicker" placeholder="YYYY-MM-DD" name="FromDate" value="@ViewBag.FromDate" readonly>
                                    </div>
                                    <div class="input-field-common">
                                        <label class="block mb-[12px] text-sm font-medium text-secondary-100 ">
                                            @CRS.CLUB.APPLICATION.Resources.Resource.From_date
                                        </label>
