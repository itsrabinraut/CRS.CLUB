                                            Total Amount
                                            <div class="relative pl-[5px]">
                                                <i class="fa-solid fa-chevron-up text-[8px] absolute top-[2px] text-[#545659]"></i>
                                                <i class="fa-solid fa-chevron-down text-[8px] absolute top-[8px] let-[5px] text-[#c3b6b9]"></i>
                                            </div>
                                        </div>
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                @{
                                    int i = 1;
                                    foreach (var item in Model.GetReservationLedgerList)
                                    {
                                        <tr class="bg-white border-b ">
                                            <td class="p-[12px]">
                                                @i
                                            </td>
                                            <td class="p-[12px]">
                                                <div class="round-image-text gap-[12px]">
                                                    <img class="w-[32px] h-[32px] rounded-full" src="@item.CustomerProfileImage" />
                                                    <div class="text-image">
                                                        @item.CustomerName
                                                        <p>@item.NickName</p>
                                                    </div>
                                                </div>
                                            </td>
                                            <td class="p-[12px]">
                                                <div class="plan-a">
                                                    @item.PlanName
                                                </div>
                                            </td>
                                            <td class="p-[12px]">
                                                @item.People

                                            </td>
                                            <td class="p-[12px]">
                                                @{
                                                    string sourceDate = item.VisitedDate;
                                                    DateTime date = DateTime.Parse(sourceDate);
                                                    string formattedDate = date.ToString("yyyy-MM-dd");
                                                }@formattedDate
                                            </td>
                                            <td class="p-[12px]">
                                                @item.VisitedTime
                                            </td>
                                            <td class="p-[12px]">
                                                @if (item.PaymentOption == "On site")
                                                {
                                                    <div class="pending">@item.PaymentOption</div>
                                                }
                                                else
                                                {
                                                    <div class="approved">@item.PaymentOption</div>
                                                }
                                            </td>
                                            <td class="p-[12px]">
                                                ¥@item.PlanAmount
                                            </td>
                                            <td class="p-[12px]">
                                                <div class="text-[#1976D2]">
                                                    ¥@item.AdminPayment
                                                </div>
                                            </td>
                                        </tr>
                                        i++;
                                    }
                                }
                            </tbody>
                        </table>
